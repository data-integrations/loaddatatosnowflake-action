{
  "metadata": {
    "spec-version": "1.0"
  },
  "configuration-groups": [
    {
      "label": "LoadDataToSnowflake Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Account Name",
          "name": "accountName"
        },
        {
          "widget-type": "textbox",
          "label": "User Name",
          "name": "userName"
        },
        {
          "widget-type": "password",
          "label": "Password",
          "name": "password"
        },
        {
          "widget-type": "textbox",
          "label": "Warehouse Name",
          "name": "warehouse"
        },
        {
          "widget-type": "textbox",
          "label": "Database Name",
          "name": "dbName"
        },
        {
          "widget-type": "textbox",
          "label": "Schema",
          "name": "schema"
        },
        {
          "widget-type": "textbox",
          "label": "Table Name",
          "name": "tableName"
        },
        {
          "widget-type": "textbox",
          "label": "Stage Name",
          "name": "stageName"
        },
        {
          "widget-type": "select",
          "label": "Data Source",
          "name": "dataSource",
          "widget-attributes": {
            "values": [
              "Internal Location",
              "External Location"
            ],
            "default":"Internal Location"
          }
        },
        {
          "widget-type": "textbox",
          "label": "File Path/S3 Bucket URL",
          "name": "path"
        },
        {
          "widget-type": "select",
          "label": "File Format Type",
          "name": "fileFormatType",
          "widget-attributes": {
            "values": [
              "CSV",
              "AVRO",
              "PARQUET",
              "JSON",
              "XML"
            ],
            "default":"CSV"
          }
        },
        {
          "widget-type": "select",
          "label": "S3 Authentication Method",
          "name": "s3AuthenticationMethod",
          "widget-attributes": {
            "values": [
               "Access Credentials",
               "IAM"
            ],
           "default":"Access Credentials"
          }
        },
        {
          "widget-type": "password",
          "label": "S3 Access Key",
          "name": "accessKey"
        },
        {
          "widget-type": "password",
          "label": "S3 Secret Access Key",
          "name": "secretAccessKey"
        }
      ]
    }
  ],
  "outputs": []
}

